{
  "name": "brah-weiroll-sdk",
  "version": "1.1.0",
  "description": "Brahma's Weiroll Development SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "prepare": "npm run build",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Brahma-fi/brah-weiroll-sdk"
  },
  "keywords": [
    "brahma.fi",
    "sdk",
    "solidity",
    "contracts",
    "weiroll",
    "ethers",
    "evm"
  ],
  "author": "pradeep-selva",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Brahma-fi/brah-weiroll-sdk/issues"
  },
  "homepage": "https://github.com/Brahma-fi/brah-weiroll-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/jest": "28.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.17.0",
    "eslint-plugin-jest": "26.5.3",
    "ethers": "^5.7.0",
    "hardhat": "2.9.1",
    "jest": "28.1.0",
    "prettier": "2.6.2",
    "ts-jest": "28.0.4",
    "typescript": "4.7.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@weiroll/weiroll.js": "^0.3.0"
  }
}
